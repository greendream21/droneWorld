    "jest": "22.0.4",
    "keyboardjs": "^2.4.1",
    "ndarray": "^1.0.18",
    "nipplejs": "^0.6.8",
    "object-assign": "4.1.1",
    "pointer-lock": "^0.0.4",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.9",
    "promise": "8.0.1",
    "pubsub-js": "^1.5.8",
    "query-string": "^5.0.1",
    "raf": "3.4.0",
    "react": "^16.3.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "resolve": "1.6.0",
    "shader-particle-engine": "git+ssh://git@github.com:blaze33/ShaderParticleEngine.git#d2df4b8a9ee97af8f282daa61911e47cff86d488",
    "simplex-noise": "^2.4.0",
    "stats.js": "^0.17.0",
    "style-loader": "0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "three": "0.92.0",
    "three.terrain.js": "^1.5.0",
    "upng-js": "^2.1.0",
